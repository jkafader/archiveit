from django.contrib import admin
from archiveit.models import Collection, Seed, SeedGroup, SeedFolder, SeedScopeRule, Crawl, Comment

# Register your models here.
class CollectionAdmin():
    class Meta:
        model = Collection

class SeedAdmin():
    class Meta:
        model = Seed

class SeedGroupAdmin():
    class Meta:
        model = SeedGroup

class SeedFolderAdmin():
    class Meta:
        model = SeedFolder

class SeedScopeRuleAdmin():
    class Meta:
        model = SeedScopeRule

class CrawlAdmin():
    class Meta:
        model = Crawl

class CommentAdmin():
    class Meta:
        model = Comment

